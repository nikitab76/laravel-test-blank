{
    "name": "nikitab76/laravel-test-blank",
    "type": "project",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9"
